$(function(){ 
   $(".hs-modal-close").click(function(){
	  $(".hs-fade-inquiry-fast").removeClass("done-show"); 
	});  
});
function get_a_quote(pid){
	if(pid!==""){
	 $("#productid").val(pid);	
	}
	$(".hs-fade-inquiry-fast").addClass("done-show");	
}
</script>
  <script src="../js/jquery.jqzoom.min.js"></script>
  <script type="text/javascript">
    $(document).ready(function () {
      $.get("https://www.honeyeheater.com/lib/pageview.php?t=2&id=57", { stamp: Math.random() }, function (data) { $(".viewnum").html(data); });

      $('.imglist li').mouseover(function () {
        var imgsrc = $(this).find('img').attr('src');
        $(this).addClass('current').siblings().removeClass('current');
        $('.picture .bigpicture img').attr('src', imgsrc);
        $('.picture .bigpicture img').attr('jqimg', imgsrc);
      });
      $('.previous').click(function () {
        var index = $('.imglist .current').index();
        var listwidht = $('.imglist').width();
        var liwidth = $('.imglist li').width() + 12;
        if (index == 0) {
          return false;
        } else {
          $('.imglist li').eq(index - 1).addClass('current').siblings().removeClass('current');
          var imgsrc = $('.imglist li').eq(index - 1).find('img').attr('src')
          $('.picture .bigpicture img').attr('src', imgsrc);
          $('.picture .bigpicture img').attr('jqimg', imgsrc);
        }
        if (index < Math.ceil(listwidht / liwidth)) {
          $('.imglist ul').animate({
            'margin-left': 0
          }, 300)
        }
      });
      $('.next').click(function () {
        var index = $('.imglist .current').index();
        var length = $('.imglist li').length;
        var listwidht = $('.imglist').width();
        var liwidth = $('.imglist li').width() + 12;
        if ((index + 1) == length) {
          return false;
        } else {
          $('.imglist li').eq(index + 1).addClass('current').siblings().removeClass('current');
          var imgsrc = $('.imglist li').eq(index + 1).find('img').attr('src')
          $('.picture .bigpicture img').attr('src', imgsrc);
          $('.picture .bigpicture img').attr('jqimg', imgsrc);
        }
        if ((index + 2) * liwidth > listwidht) {
          var marginwidth = '-' + ((index + 2) * liwidth - listwidht);
          $('.imglist ul').animate({
            'margin-left': marginwidth
          }, 300)
        }
      });

      $(".jqzoom").jqueryzoom({
					xzoom: 500,
					yzoom: 500,
					offset: 10,
					position: "right",
					preload: 1,
					lens: 1
				});

        $('.productdetailtabli li').click(function(){
          var thisIndex = $(this).index();
          $(this).addClass('current').siblings().removeClass('current');
          $('.productdetailtabcon').eq(thisIndex).addClass('show').siblings().removeClass('show');
        });
    });
  </script>
</body>

</html>