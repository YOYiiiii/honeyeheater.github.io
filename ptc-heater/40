  <div class="productdetail">
    <div class="container">
      
      <div class="contentdetail">
        <div class="picture">
          <div class="bigpicture jqzoom"><img src="../upload/20241109/140339_90122.jpg" alt="PTC Element" jqimg="https://www.honeyeheater.com/upload/20241109/140339_90122.jpg" title="PTC Element"></div>
          <div class="thumbnail">
            <div class="button"><a href="javascript:void(0);" class="previous">&lt;</a></div>
