<script type="text/javascript">
    $('.toolstab .nav').click(function(){
        var thisIndex = $(this).index();
        $(this).addClass('current').siblings().removeClass('current');
        $('.toolslist .item').eq(thisIndex).addClass('show').siblings().removeClass('show');
    });

    $('.calculate1').click(function(){
        var $thisItem = $(this).parents('.item');
        var specific = parseFloat($thisItem.find('.specific').val());
        var acreage = parseFloat($thisItem.find('.acreage').val());
        var length = parseFloat($thisItem.find('.length').val());
        
        if (isNaN(specific) || isNaN(acreage) || isNaN(length)) {
            $thisItem.find('.tips').show().text("Please enter a valid value!");
            return;
        }
        var ohm = specific / acreage * length;
        $thisItem.find('.tips').hide();
        $thisItem.find('.result').show();
        $thisItem.find('.ohm').text(ohm.toFixed(2) + ' Ohm');
    })

    $('.calculate2').click(function(){
        var $thisItem = $(this).parents('.item');
        var voltage = parseFloat($thisItem.find('.voltage').val());
        var resistance = parseFloat($thisItem.find('.resistance').val());
        
        if (isNaN(voltage) || isNaN(resistance)) {
            $thisItem.find('.tips').show().text("Please enter a valid value!");
            return;
        }
        var current = voltage / resistance;
        var power = (current * voltage).toFixed(2);
        $thisItem.find('.tips').hide();
        $thisItem.find('.result').show();
        $thisItem.find('.electricity').text(current.toFixed(2) + ' Ampere');
        $thisItem.find('.power').text(power + ' Watts');
    })
    
    $('.calculate3').click(function(){
        var $thisItem = $(this).parents('.item');
        var voltage = parseFloat($thisItem.find('.voltage').val());
        var resistance = parseFloat($thisItem.find('.resistance').val());
        var time = parseFloat($thisItem.find('.time').val());
        
        if (isNaN(voltage) || isNaN(resistance)) {
            $thisItem.find('.tips').show().text("Please enter a valid value!");
            return;
        }
        var current = voltage / resistance;
        var power = (current * voltage).toFixed(2);
        var electric = power * time;
        $thisItem.find('.tips').hide();
        $thisItem.find('.result').show();
        $thisItem.find('.electricity').text(current.toFixed(2) + ' Ampere');
        $thisItem.find('.power').text(power + ' Watts');
        $thisItem.find('.electric').text(electric.toFixed(2) + ' Joule')
    })
    
    $('.calculate4').click(function(){
        var $thisItem = $(this).parents('.item');
        var quantity = parseFloat($thisItem.find('.quantity').val());
        var temperature = parseFloat($thisItem.find('.temperature').val());
        var quality = parseFloat($thisItem.find('.quality').val());
        
        if (isNaN(quantity) || isNaN(temperature) || isNaN(quality)) {
            $thisItem.find('.tips').show().text("Please enter a valid value!");
            return;
        }
        var capacity = quantity / temperature / quality;
        $thisItem.find('.tips').hide();
        $thisItem.find('.result').show();
        $thisItem.find('.capacity').text(capacity.toFixed(2));
    })
    
    $('.calculate5').click(function(){
        var $thisItem = $(this).parents('.item');
        var temperature = parseFloat($thisItem.find('.temperature').val());
        
        if (isNaN(temperature)) {
            $thisItem.find('.tips').show().text("Please enter a valid value!");
            return;
        }
        if (temperature <= 15) {
            $thisItem.find('.tips').show().text("Please enter a valid value!");
            return;
        }
        var thermalenergy = 1.29 * 0.25 * (temperature - 15);
        $thisItem.find('.tips').hide();
        $thisItem.find('.result').show();
        $thisItem.find('.thermalenergy').text(thermalenergy.toFixed(2) + ' Joule');
    })
    
    $('.calculate6').click(function(){
        var $thisItem = $(this).parents('.item');
        var water = parseFloat($thisItem.find('.water').val());
        var stemperature = parseFloat($thisItem.find('.s-temperature').val());
        var etemperature = parseFloat($thisItem.find('.e-temperature').val());
        var time = parseFloat($thisItem.find('.time').val());
        
        if (isNaN(water) || isNaN(stemperature) || isNaN(etemperature) || isNaN(time)) {
            $thisItem.find('.tips').show().text("Please enter a valid value!");
            return;
        }
        if (etemperature <= stemperature) {
            $thisItem.find('.tips').show().text("Please enter a valid value!");
            return;
        }
        var power = 4200 * water * (etemperature - stemperature) / (time * 60);
        $thisItem.find('.tips').hide();
        $thisItem.find('.result').show();
        $thisItem.find('.power').text(power + ' Watts');
    })
</script>									</div>
			</div>
		</div>
	</div>


